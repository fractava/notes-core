{
  "name": "dev-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "eslint": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "eslint:fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "stylelint": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "stylelint:fix": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore --fix",
    "lint": "npm run eslint && npm run stylelint"
  },
  "dependencies": {
    "@nuxtjs/pwa": "^3.3.5",
    "core-js": "^3.9.1",
    "eslint-config-jonathantreffler": "^1.5.1",
    "nuxt": "^2.15.3",
    "stylelint-config-jonathantreffler": "^1.1.0",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.22.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.7.0",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0"
  }
}
